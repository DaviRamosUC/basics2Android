<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="@dimen/general_padding"
    tools:context=".ui.clientes.ClientesFragment">

    <TextView
        android:id="@+id/textView2"
        style="@style/Title_about"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/menu_clientes" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:text="@string/clientes_text"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/textView8"
        android:text="Cielo"
        style="@style/clientes_style" />

    <TextView
        android:id="@+id/textView9"
        style="@style/clientes_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Brasken" />

    <TextView
        android:id="@+id/textView10"
        style="@style/clientes_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Embraer" />

    <TextView
        android:id="@+id/textView11"
        style="@style/clientes_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Natura" />

    <TextView
        android:id="@+id/textView12"
        style="@style/clientes_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Lojas Americanas" />

</LinearLayout>